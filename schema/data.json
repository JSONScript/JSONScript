{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "id": "http://jsonscript.net/schema/data.json",
    "title": "Data schema",
    "description": "Allowed data inside JSONScript (no first $ in keys and strings)",
    "anyOf": [
        { "type": "object",
          "patternProperties": { "^[^$]|^$": { "$ref": "#" } },
          "additionalProperties": false },
        { "type": "array", "items": { "$ref": "#" } },
        { "$ref": "_defs.json#/defs/dataString" },
        { "type": [ "number", "boolean", "null" ] }
    ]
}
