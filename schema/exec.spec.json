[ 
  { "it": { "$exec": "Router", "$method": "get" }, "isValid": true },
  { "it": { "$exec": "Router", "$method": "GET" }, "isValid": true },
  { "it": { "$exec": "router", "$method": "get" }, "isValid": false },
  { "it": { "$exec": "Router" }, "isValid": false },
  { "it":
    { "$exec": "Router",
      "$method": "get",
      "$args": "/resourse/1" },
    "isValid": true },
  { "it":
    { "$exec": "Router",
      "$method": "get",
      "$args": [ "/resourse/1" ] },
    "isValid": true },
  { "it":
    { "$exec": "Router",
      "$method": "get",
      "$args": { "url": "/resourse" } },
    "isValid": true }
]
